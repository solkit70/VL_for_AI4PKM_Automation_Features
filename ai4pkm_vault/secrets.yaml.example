# Secrets Configuration
# This file overrides values in orchestrator.yaml
# Copy this file to secrets.yaml and fill in your actual secrets
# secrets.yaml is gitignored and should not be committed

# Pollers secrets
pollers:
  gobi:
    api_key: "your-gobi-api-key-here"
    
  gobi_by_tags:
    admin_api_key: "your-gobi-admin-api-key-here"
    tags: "your-tags-here"  # optional
    
  limitless:
    api_key: "your-limitless-api-key-here"

