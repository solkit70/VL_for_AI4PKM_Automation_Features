views:
  - type: table
    name: Table
    filters:
      and:
        - file.path.startsWith("AI/Tasks/")
        - file.ext == "md"
        - '!file.path.startsWith("AI/Tasks/Logs")'
        - '!file.path.startsWith("AI/Tasks/Requests")'
    order:
      - file.name
      - created
      - status
      - priority
      - worker
      - task_type
    sort:
      - property: created
        direction: DESC
    columnSize:
      file.name: 335
      note.created: 159
      note.status: 155
