views:
  - type: table
    name: Table
    filters:
      and:
        - file.path.startsWith("_Settings_/Tasks")
        - file.ext == "md"
        - status != "IGNORE"
    order:
      - file.name
      - output
      - created
      - status
      - priority
      - worker
      - task_type
    sort:
      - property: created
        direction: DESC
    columnSize:
      file.name: 372
      note.output: 381
      note.created: 159
      note.status: 155
